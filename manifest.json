{
  "manifest_version": 2,

  "name": "Note Quote",
  "description": "Jot down inspirational thoughts by others including yourself.",
  "version": "1.0",

  "chrome_url_overrides" : {
    "newtab": "index.html"
  },

  "permissions": [
    "storage"
  ],

  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"

}